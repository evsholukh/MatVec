{
    "tasks": [
        {
            "label": "Run Bench",
            "command": "python",
            "args": [
                "bench.py"
            ],
            "problemMatcher": []
        },
        {
            "label": "Run Vector",
            "command": "vector.exe",
            "args": [
                "--all"
            ],
            "dependsOrder": "sequence",
            "dependsOn": [
                "C/C++: Build Vector Release"
            ],
            "problemMatcher": []
        },
        {
            "label": "Run Matrix",
            "command": "matrix.exe",
