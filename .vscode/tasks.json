{
    "tasks": [
        {
            "label": "Launch Python",
            "command": "python",
            "args": [
                "bench.py"
            ],
            "problemMatcher": []
        },
        {
            "label": "Launch Dot",
            "command": "dot.exe",
            "args": [
                "--all",
                "--float"
            ],
            "dependsOrder": "sequence",
            "dependsOn": [
                "C/C++: Build Dot Release"
            ],
            "problemMatcher": []
        },
        {
            "label": "Launch GEMM",
            "command": "gemm.exe",
            "dependsOrder": "sequence",
            "args": ["-k", "20000", "--opencl", "--clblast", "--float"],
            "dependsOn": [
                "C/C++: Build GEMM Release"
            ],
            "problemMatcher": []
        },
        {
            "label": "Git Clean",
            "command": "git",
            "args": [
                "clean",
                "-f",
                "-x"
            ],
            "group": "none",
            "problemMatcher": []
        },
        {
            "label": "Build All",
            "dependsOrder": "parallel",
            "dependsOn": [
                "C/C++: Build Dot Debug",
                "C/C++: Build Dot Release",
                "C/C++: Build GEMM Debug",
                "C/C++: Build GEMM Release"
            ],
            "problemMatcher": []
        },
        {
            "label": "Build All CUDA",
            "dependsOrder": "sequence",
            "dependsOn": [
                "C/C++: Build Dot CUDA Debug",
                "C/C++: Build Dot CUDA Release",
                "C/C++: Build GEMM CUDA Debug",
                "C/C++: Build GEMM CUDA Release"
            ],
            "problemMatcher": []
        },
        {
            "type": "cppbuild",
            "label": "C/C++: Build Dot Release",
            "command": "g++.exe",
            "args": [
                "${workspaceFolder}\\dot.cpp",
                "-o",
                "${workspaceFolder}\\dot.exe",
                "-I",
                "${workspaceFolder}",
                "-I",
                "C:\\3d\\OpenCL-SDK-v2024.10.24-Win-x64\\include",
                "-I",
                "C:\\3d\\CLBlast-1.6.3-windows-x64\\include",
                "-I",
                "C:\\3d\\OpenBLAS-0.3.29_x64\\include",
                "-L",
                "C:\\3d\\OpenCL-SDK-v2024.10.24-Win-x64\\lib",
                "-L",
                "C:\\3d\\CLBlast-1.6.3-windows-x64\\lib",
                "-L",
                "C:\\3d\\OpenBLAS-0.3.29_x64\\lib",
                "-fopenmp",
                "-lopencl",
                "-lopenblas",
                "-lclblast",
                "-std=c++17",
                "-O3",
                "-DOPT_LEVEL='\"-O3\"'"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "type": "cppbuild",
            "label": "C/C++: Build Dot Debug",
            "command": "g++.exe",
            "args": [
                "-g",
                "${workspaceFolder}\\dot.cpp",
                "-o",
                "${workspaceFolder}\\dot_debug.exe",
                "-I",
                "${workspaceFolder}",
                "-I",
                "C:\\3d\\OpenCL-SDK-v2024.10.24-Win-x64\\include",
                "-I",
                "C:\\3d\\CLBlast-1.6.3-windows-x64\\include",
                "-I",
                "C:\\3d\\OpenBLAS-0.3.29_x64\\include",
                "-L",
                "C:\\3d\\OpenCL-SDK-v2024.10.24-Win-x64\\lib",
                "-L",
                "C:\\3d\\CLBlast-1.6.3-windows-x64\\lib",
                "-L",
                "C:\\3d\\OpenBLAS-0.3.29_x64\\lib",
                "-fopenmp",
                "-lopencl",
                "-lopenblas",
                "-lclblast",
                "-std=c++17"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "type": "cppbuild",
            "label": "C/C++: Build GEMM Release",
            "command": "g++.exe",
            "args": [
                "${workspaceFolder}\\gemm.cpp",
                "-o",
                "${workspaceFolder}\\gemm.exe",
                "-I",
                "${workspaceFolder}",
                "-I",
                "C:\\3d\\OpenCL-SDK-v2024.10.24-Win-x64\\include",
                "-I",
                "C:\\3d\\CLBlast-1.6.3-windows-x64\\include",
                "-I",
                "C:\\3d\\OpenBLAS-0.3.29_x64\\include",
                "-L",
                "C:\\3d\\OpenCL-SDK-v2024.10.24-Win-x64\\lib",
                "-L",
                "C:\\3d\\CLBlast-1.6.3-windows-x64\\lib",
                "-L",
                "C:\\3d\\OpenBLAS-0.3.29_x64\\lib",
                "-fopenmp",
                "-lopencl",
                "-lopenblas",
                "-lclblast",
                "-std=c++17",
                "-O3",
                "-DOPT_LEVEL='\"-O3\"'"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "type": "cppbuild",
            "label": "C/C++: Build GEMM Debug",
            "command": "g++.exe",
            "args": [
                "-g",
                "${workspaceFolder}\\gemm.cpp",
                "-o",
                "${workspaceFolder}\\gemm_debug.exe",
                "-I",
                "${workspaceFolder}",
                "-I",
                "C:\\3d\\OpenCL-SDK-v2024.10.24-Win-x64\\include",
                "-I",
                "C:\\3d\\CLBlast-1.6.3-windows-x64\\include",
                "-I",
                "C:\\3d\\OpenBLAS-0.3.29_x64\\include",
                "-L",
                "C:\\3d\\OpenCL-SDK-v2024.10.24-Win-x64\\lib",
                "-L",
                "C:\\3d\\CLBlast-1.6.3-windows-x64\\lib",
                "-L",
                "C:\\3d\\OpenBLAS-0.3.29_x64\\lib",
                "-fopenmp",
                "-lopencl",
                "-lopenblas",
                "-lclblast",
                "-std=c++17",
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "type": "cppbuild",
            "label": "C/C++: Build Dot CUDA Release",
            "command": "nvcc.exe",
            "args": [
                "dot.cu",
                "-o",
                "dot_cuda.exe",
                "-I",
                "${env:CUDA_PATH_V12_4}\\include",
                "-L",
                "${env:CUDA_PATH_V12_4}\\lib\\x64",
                "-lcudart",
                "-lcublas",
                "-allow-unsupported-compiler",
                "-std=c++17",
                "-O3"
            ]
        },
        {
            "type": "cppbuild",
            "label": "C/C++: Build Dot CUDA Debug",
            "command": "nvcc.exe",
            "args": [
                "dot.cu",
                "-g",
                "-o",
                "dot_cuda_debug.exe",
                "-I",
                "${env:CUDA_PATH_V12_4}\\include",
                "-L",
                "${env:CUDA_PATH_V12_4}\\lib\\x64",
                "-lcudart",
                "-lcublas",
                "-allow-unsupported-compiler",
                "-std=c++17"
            ]
        },
        {
            "type": "cppbuild",
            "label": "C/C++: Build GEMM CUDA Release",
            "command": "nvcc.exe",
            "args": [
                "gemm.cu",
                "-o",
                "gemm_cuda.exe",
                "-I",
                "${env:CUDA_PATH_V12_4}\\include",
                "-L",
                "${env:CUDA_PATH_V12_4}\\lib\\x64",
                "-lcudart",
                "-lcublas",
                "-allow-unsupported-compiler",
                "-std=c++17",
                "-O3"
            ]
        },
        {
            "type": "cppbuild",
            "label": "C/C++: Build GEMM CUDA Debug",
            "command": "nvcc.exe",
            "args": [
                "gemm.cu",
                "-g",
                "-o",
                "gemm_cuda_debug.exe",
                "-I",
                "${env:CUDA_PATH_V12_4}\\include",
                "-L",
                "${env:CUDA_PATH_V12_4}\\lib\\x64",
                "-lcudart",
                "-lcublas",
                "-allow-unsupported-compiler",
                "-std=c++17"
            ]
        }
    ],
    "version": "2.0.0"
}