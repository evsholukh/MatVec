            "args": [
                "--all"
            ],
            "dependsOn": [
                "C/C++: Build Matrix Release"
            ],
            "problemMatcher": []
        },
        {
            "label": "Clean",
            "command": "git",
            "args": [
                "clean",
                "-f",
                "-x"
            ],
            "group": "none",
            "problemMatcher": []
        },
        {
            "label": "Build All",
            "dependsOrder": "parallel",
            "dependsOn": [
                "C/C++: Build Vector Debug",
                "C/C++: Build Vector Release",
                "C/C++: Build Matrix Debug",
                "C/C++: Build Matrix Release",
            ],
            "problemMatcher": []
        },
        {
            "label": "Build All CUDA",
            "dependsOrder": "sequence",
            "dependsOn": [
                "C/C++: Build Vector CUDA Debug",
                "C/C++: Build Vector CUDA Release",
                "C/C++: Build Matrix CUDA Debug",
                "C/C++: Build Matrix CUDA Release",
            ],
            "problemMatcher": []
        },
        {
            "type": "cppbuild",
            "label": "C/C++: Build Vector Release",
            "command": "g++.exe",
            "args": [
                "${workspaceFolder}\\vector.cpp",
                "-o",
                "${workspaceFolder}\\vector.exe",
                "-I",
                "${workspaceFolder}",
                "-I",
                "C:\\3d\\OpenCL-SDK-v2024.10.24-Win-x64\\include",
                "-I",
                "C:\\3d\\CLBlast-1.6.3-windows-x64\\include",
                "-I",
                "C:\\3d\\OpenBLAS-0.3.29_x64\\include",
                "-L",
                "C:\\3d\\OpenCL-SDK-v2024.10.24-Win-x64\\lib",
                "-L",
                "C:\\3d\\CLBlast-1.6.3-windows-x64\\lib",
                "-L",
                "C:\\3d\\OpenBLAS-0.3.29_x64\\lib",
                "-fopenmp",
                "-lopencl",
                "-lopenblas",
                "-lclblast",
                "-std=c++17",
                "-O3",
                "-DOPT_LEVEL='\"-O3\"'",
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
